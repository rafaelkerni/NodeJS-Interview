{
    "source": {
      "include": ["src"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|.history/|.vscode/)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "./docs/",
      "template": "./docdash"
    },
    "docdash": {
      "static": [true],        
      "sort": [false],         
      "sectionOrder": [               
           "Classes",
           "Modules",
           "Externals",
           "Events",
           "Namespaces",
           "Mixins",
           "Tutorials",
           "Interfaces"
      ],
      "disqus": "",                   
      "openGraph": {                 
          "title": "Documentação NodeJS-Interview",               
          "type": "website",         
          "image": "",              
          "site_name": "",          
          "url": ""                   
      },
      "meta": {                      
          "title": "",                
          "description": "",          
          "keyword": ""               
      },
      "search": [true],         
      "collapse": [true],       
      "wrap": [true],          
      "typedefs": [true],      
      "navLevel": [0],          
      "private": [true],        
      "removeQuotes": [],
      "scripts": [],                 
      "scopeInOutputPath": [true], 
      "nameInOutputPath": [true],
      "versionInOutputPath": [true] 
  }
}